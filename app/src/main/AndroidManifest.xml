<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dat.swipe_example">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:targetApi="31">
        <activity
            android:name=".navigation_manager.gridtopager.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SwipeableFragmen.NoActionBar">

        </activity>
        <activity
            android:name=".activity_navigation.Activity1"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SwipeableFragmen.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity_navigation.Activity2"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>
    </application>

</manifest>